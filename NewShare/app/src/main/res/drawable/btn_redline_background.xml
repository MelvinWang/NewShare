<?xml version="1.0" encoding="UTF-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="0.0px"
    android:insetRight="0.0px">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <corners android:radius="100dp" />
                <solid android:color="@color/f2f2f2" />
                <stroke
                    android:width="1dp"
                    android:color="@color/color_999" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="@color/f2f2f2" />
                <corners android:radius="100dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/app_common_red" />
            </shape>
        </item>
    </selector>

</inset>